{
  "name": "sasqchat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "tsx scripts/seed.ts",
    "export": "next build && next export -o out",
    "cap:add": "npx @capacitor/cli add ios",
    "cap:sync": "npx @capacitor/cli sync ios",
    "cap:open": "npx @capacitor/cli open ios",
    "ios": "npm run export && npm run cap:sync && npm run cap:open"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "dependencies": {
    "@capacitor/app": "^7.1.0",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@livekit/components-react": "^2.9.16",
    "@livekit/components-styles": "^1.2.0",
    "@prisma/client": "^6.15.0",
    "jsonwebtoken": "^9.0.2",
    "livekit-client": "^2.15.6",
    "livekit-server-sdk": "^2.13.3",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "simli-client": "^1.2.12",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
